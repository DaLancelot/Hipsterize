{
  "name": "new",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kingcosmic.github.io/tinyquotes",
  "dependencies": {
    "custom-react-scripts": "^0.2.2",
    "gh-pages": "^1.1.0",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/scss/index.scss src/css/index.css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss/index.scss src/css/index.css --watch --recursive",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
